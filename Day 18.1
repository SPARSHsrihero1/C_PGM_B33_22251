#include <stdio.h>
/*
 * Program to print all factors of a given number.
 * A factor is a number that divides the given number exactly without leaving a remainder.
 */

int main() {
    int num, i;

    printf("Enter a number: ");
    scanf("%d", &num);

    printf("Factors of %d are:\n", num);
    for (i = 1; i <= num; i++) {
        if (num % i == 0) {
            printf("%d\n", i);
        }
    }

    return 0;
}
