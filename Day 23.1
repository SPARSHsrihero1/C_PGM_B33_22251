#include <stdio.h>
/*Program to find the sum of the series:2/3 + 4/7 + 6/11 + 8/15 + ... up to n terms
int main() {
    int n, k;
    float term, sum = 0;

    printf("Enter number of terms: ");
    scanf("%d", &n);

    for (k = 1; k <= n; k++) {
        term = (float)(2 * k) / (4 * k - 1);
        sum += term;
    }

    printf("Sum of the series up to %d terms is: %.4f\n", n, sum);

    return 0;
}
