#include <stdio.h>

/*
    Question:
    Write a C program to print all prime numbers from 1 to n
*/

int main() {
    int n;
    printf("Enter the value of n: ");
    scanf("%d", &n);

    printf("Prime numbers from 1 to %d are:\n", n);

    for (int num = 2; num <= n; num++) {
        int isPrime = 1;

        if (num == 2 || num == 3) {
            isPrime = 1;
        } else if (num % 2 == 0 || num % 3 == 0) {
            isPrime = 0;
        } else {
            int i = 5;
            while (i * i <= num) {
                if (num % i == 0 || num % (i + 2) == 0) {
                    isPrime = 0;
                }
                i += 6;
            }
        }

        if (isPrime == 1) {
            printf("%d ", num);
        }
    }

    printf("\n");
    return 0;
}
