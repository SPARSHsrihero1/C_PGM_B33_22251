#include <stdio.h>

/*Program to print the following pattern:

   *
   
   *
   *
   *
   
   *
   *
   *
   *
   *
   
   *
   *
   *
   
   *
*/

int main() {
    int i;

    printf("*\n\n");

    for (i = 0; i < 3; i++) {
        printf("*\n");
    }
    printf("\n");

    for (i = 0; i < 5; i++) {
        printf("*\n");
    }
    printf("\n");

    for (i = 0; i < 3; i++) {
        printf("*\n");
    }
    printf("\n");

    printf("*\n");

    return 0;
}
